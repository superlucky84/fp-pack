{
  "name": "fp-pack",
  "version": "0.9.1",
  "type": "module",
  "description": "Functional programming utilities library for TypeScript",
  "keywords": [
    "functional",
    "functional-programming",
    "fp",
    "typescript",
    "javascript",
    "utility",
    "utilities",
    "pipe",
    "compose",
    "curry",
    "stream",
    "iterable",
    "async",
    "lazy",
    "side-effect"
  ],
  "license": "MIT",
  "homepage": "https://superlucky84.github.io/fp-pack/",
  "repository": "https://github.com/superlucky84/fp-pack",
  "bugs": {
    "url": "https://github.com/superlucky84/fp-pack/issues"
  },
  "main": "./dist/fp-pack.umd.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/fp-pack.umd.js"
    },
    "./stream": {
      "types": "./dist/stream/index.d.ts",
      "import": "./dist/stream/index.mjs",
      "require": "./dist/fp-pack-stream.umd.js"
    }
  },
  "typesVersions": {
    "*": {
      "stream": [
        "dist/stream/index.d.ts"
      ],
      "stream/*": [
        "dist/stream/*"
      ]
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE",
    "dist/ai-addons",
    "dist/skills"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build && vite build --config vite.umd.config.ts && vite build --config vite.umd.stream.config.ts && node scripts/copy-skills.mjs",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "pnpm test:types && vitest run",
    "test:types": "tsc --noEmit",
    "clean": "pnpm clean:dist && pnpm clean:module",
    "clean:dist": "rm -rf dist docs/dist",
    "clean:module": "rm -rf dist docs/dist node_modules",
    "docs:dev": "pnpm --filter docs dev",
    "docs:build": "pnpm --filter docs build",
    "docs:preview": "pnpm --filter docs preview",
    "docs:lint": "pnpm --filter docs lint",
    "docs:lint:fix": "pnpm --filter docs lint:fix",
    "docs:format": "pnpm --filter docs format"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@vitest/ui": "2.1.9",
    "prettier": "^3.7.4",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.9"
  }
}
