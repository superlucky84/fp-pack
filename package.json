{
  "name": "fp-kit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "Functional programming utilities library for TypeScript",
  "main": "./dist/fp-kit.umd.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/fp-kit.umd.js"
    },
    "./monad": {
      "types": "./dist/monad/index.d.ts",
      "import": "./dist/monad/index.mjs"
    },
    "./stream": {
      "types": "./dist/stream/index.d.ts",
      "import": "./dist/stream/index.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "monad": ["dist/monad/index.d.ts"],
      "monad/*": ["dist/monad/*"],
      "stream": ["dist/stream/index.d.ts"],
      "stream/*": ["dist/stream/*"]
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "docs:dev": "pnpm --filter docs dev",
    "docs:build": "pnpm --filter docs build",
    "docs:preview": "pnpm --filter docs preview",
    "docs:lint": "pnpm --filter docs lint",
    "docs:lint:fix": "pnpm --filter docs lint:fix",
    "docs:format": "pnpm --filter docs format"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@vitest/ui": "2.1.9",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.9"
  }
}
